<p style="color: green"><%= notice %></p>

<%= render @member , desde:@desde %>

<div class="btn-group">
  <%= link_to "Editar usuario", edit_member_path(@member), class:"btn btn-success btn-sm" %> &nbsp;|&nbsp;
  <%= link_to "ver Todos", members_path, class:"btn btn-success btn-sm" %>
</div>
<% if @member.id != 8 %>
  <%= button_to "Eliminar usuario", @member, method: :delete, class:"btn btn-danger btn-sm mt-5" %>
<% end %>


