<p style="color: green"><%= notice %></p>

<% content_for :title, "Members" %>

<h1>Usuarios Registrados</h1>
<div class="container-fluid">
  <%= link_to "Agregar Usuario", new_member_path, class:"btn btn-success btn-sm mb-3" %>
  <div id="members" class="row">
    <% @members.each do |member| %>
      <%= render member, desde:@desde %>
      
    <% end %>
    <div class="col-12">
      <%= paginate @members %>
    </div>
  </div>
  <%= link_to "Agregar Usuario", new_member_path, class:"btn btn-success btn-sm mt-3" %>
</div>

